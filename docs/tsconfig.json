{
  "compilerOptions": {
    "outDir": "./dist/",
    "baseUrl": ".src",
    "sourceMap": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "module": "ESNext",
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "jsx": "react-jsx",
    "paths": {
      "@dist/*": ["../../dist/*"],
    }

  },
  "include": ["src"],
  "
}
